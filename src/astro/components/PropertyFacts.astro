---
const { listing } = Astro.props;
---
<div class="property-facts">
  <h2>PROPERTY FACTS</h2>
  <div class="facts-container">
    {listing.property_facts.map((fact) => (
      <div class="fact-item">
        <span class="fact-label">{fact.label}:</span>
        <span class="fact-value">{fact.value}</span>
      </div>
    ))}
  </div>
</div>
