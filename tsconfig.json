{
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "src/types/**/*.d.ts"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "target": "ES6",
    "module": "ESNext",
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/*"]
    }
  }
}